<launch>
  <master auto="start"/>

  <!-- Run the map server -->
<!--
  <node name="map_server" pkg="map_server" type="map_server" args="$(find my_map_package)/my_map.pgm my_map_resolution"/>
-->

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <remap from="cmd_vel" to="rosaria/cmd_vel"/>
    <rosparam file="$(find p3dx_launch)/nav_config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find p3dx_launch)/nav_config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find p3dx_launch)/nav_config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find p3dx_launch)/nav_config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find p3dx_launch)/nav_config/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
